	<Frame name="StatCompareFrame" hidden="false" parent="UIParent">
		<Scripts>
			<OnLoad>
				this:RegisterEvent("VARIABLES_LOADED");
			</OnLoad>
			<OnEvent>
				StatCompare_OnEvent();
			</OnEvent>
